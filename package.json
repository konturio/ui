{
  "name": "k2-front-end",
  "workspaces": [
    "k2-packages/*",
    "k2-dev/*"
  ],
  "private": true,
  "scripts": {
    "clean": "lerna clean --yes",
    "bootstrap": "yarn fix:dependencies && lerna bootstrap && lerna link",
    "dev": "run-p start test:watch",
    "start": "yarn bootstrap && lerna run dev --stream",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test": "jest",
    "build": "tsc -b && lerna run build --stream",
    "lerna:publish": "lerna publish from-package --yes",
    "plop:new": "npx plop new",
    "plop:add": "npx plop add",
    "plop:route": "npx plop route",
    "plop:ref": "npx plop ref",
    "plop:link": "npx plop link",
    "fix:dependencies": "npx syncpack fix-mismatches --peer"
    "lint:ts": "yarn tslint -c tslint.json -p tsconfig-lint.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
