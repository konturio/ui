{
  "name": "k2-front-end",
  "workspaces": [
    "k2-packages/*",
    "k2-dev/*"
  ],
  "private": true,
  "scripts": {
    "clean": "lerna clean --yes",
    "bootstrap": "npm run fix:dependencies && lerna bootstrap && lerna link",
    "dev": "run-p start test:watch",
    "start": "npm run bootstrap && lerna run dev --stream",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test": "jest",
    "build": "tsc -b && lerna run build --stream",
    "lerna:publish": "lerna publish from-package --yes",
    "plop:new": "npx plop new",
    "plop:add": "npx plop add",
    "plop:route": "npx plop route",
    "plop:ref": "npx plop ref",
    "plop:link": "npx plop link",
    "plop:unlink": "npx plop unlink",
    "release": "lerna version --no-push",
    "postrelease": "git push && git push origin --tags",
    "fix:dependencies": "npx syncpack fix-mismatches --peer",
    "lint:ts": "eslint '*/**/*.{ts,tsx}' --quiet",
    "lint-staged": "npx --no-install lint-staged"
  },
  "lint-staged": {
    "*/**/*.{ts, tsx}": [
      "eslint --quiet --fix"
    ],
    "*/**/*.{css}": [
      "stylelint"
    ],
    "*/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@turf/difference": "6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/enzyme": "^3.10.9",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^26.0.24",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/stylus": "^0.48.36",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-i18next-extract": "^0.8.3",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.16.0",
    "css-loader": "^6.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "i18next-conv": "^10.2.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "inquirer-fuzzy-path": "^2.3.0",
    "jest": "^27.0.6",
    "jest-css-modules-transform": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-react-hooks-shallow": "^1.5.1",
    "jest-transform-stub": "^2.0.0",
    "lerna": "^3.20.2",
    "lint-staged": "^11.1.1",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^6.1.0",
    "syncpack": "^5.8.14",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
