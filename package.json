{
  "name": "k2-front-end",
  "private": true,
  "workspaces": [
    "k2-packages/*",
    "k2-apps/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "dev": "lerna run dev --stream",
    "test": "lerna run test --stream",
    "compile": "tsc --build --force tsconfig.json",
    "update:version": "lerna version --include-merged-tags",
    "create:release": "lerna version --include-merged-tags conventional-changelog -p angular -i CHANGELOG.md -w --writer-opts ./writer-opts.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^25.1.1",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-router-dom": "^5.1.2",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^25.1.0",
    "lerna": "^3.11.0",
    "prettier": "^1.16.4",
    "source-map-loader": "^0.2.3",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.2.0",
    "typescript": "^3.3.3",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.0.0"
  }
}
